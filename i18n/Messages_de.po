#: ../src/yajhfc/console/ConsCommandLineOpts.java:410
#, java-format
msgid "The value \"{0}\" specified for argument {1} is not a number."
msgstr "Der für das Argument {1} angegebene Wert \"{0}\" ist keine Zahl."

#: ../src/yajhfc/console/ConsCommandLineOpts.java:494
#, java-format
msgid "The value \"{0}\" specified for argument {1} is not a recognized date/time value."
msgstr "Der für das Argument {1} angegebene Wert \"{0}\" ist kein erkannter Datum/Uhrzeit-Wert."

#: ../src/yajhfc/console/Main.java:75
#: ../src/yajhfc/console/Main.java:80
msgid "Invalid command line arguments:"
msgstr "Ungültige Kommandozeilenargumente:"

#: ../src/yajhfc/console/Main.java:75
msgid "You cannot specify both --stdin and use batch input from stdin."
msgstr "Sie können nicht sowohl --stdin angeben als auch Batcheingaben von stdin lesen."

#: ../src/yajhfc/console/Main.java:88
msgid "You cannot both query a job status and specify documents to send."
msgstr "Sie können nicht sowohl einen Jobstatus abfragen als auch zu sendende Dokumente angeben."

#: ../src/yajhfc/console/Main.java:92
msgid "You cannot both query a job status and specify recipients."
msgstr "Sie können nicht sowohl einen Jobstatus abfragen als auch Empfänger angeben."

#: ../src/yajhfc/console/Main.java:97
msgid "You have to specify at least one recipient or query a job status or use batch mode."
msgstr "Sie müssen mindestens einen Empfänger angeben oder einen Jobstatus abfragen oder den Batchmodus benutzen."

#: ../src/yajhfc/console/Main.java:102
msgid "You cannot specify poll mode and have documents to send."
msgstr "Sie können nicht gleichzeitig den Faxabfragemodus verwenden und Dokumente versenden."

#: ../src/yajhfc/console/Main.java:141
msgid "Unsupported batch data format"
msgstr "Nicht unterstütztes Batchdatenformat"

#: ../src/yajhfc/console/Main.java:168
#: ../src/yajhfc/console/Main.java:171
msgid "Line"
msgstr "Zeile"

#: ../src/yajhfc/console/Main.java:168
msgid "Invalid arguments specified in batch mode:"
msgstr "Ungültige Argumente im Batchmodus angegeben:"

#: ../src/yajhfc/console/Main.java:171
msgid "data was"
msgstr "Daten waren:"

#: ../src/yajhfc/console/Main.java:177
#, java-format
msgid "Error processing the batch data from \"{0}\":"
msgstr "Fehler beim Verarbeiten der Batchdaten aus \"{0}\":"

#: ../src/yajhfc/console/Main.java:207
msgid "Error performing the requested operation:"
msgstr "Fehler beim Durchführen der angeforderten Aktion:"

#: ../src/yajhfc/console/Main.java:224
msgid "Sending a fax failed, exiting program."
msgstr "Das Senden eines Faxes schlug fehl, beende Programm."

#: ../src/yajhfc/console/Main.java:257
msgid "Invalid custom property"
msgstr "Ungültige benutzerdefinierte Eigenschaft"

#: ../src/yajhfc/console/Main.java:271
msgid "The kill time must be in the future; using default instead."
msgstr "Die Abbruchzeit (kill time) muss in der Zukunft liegen; verwende den Standardwert stattdessen."

#: ../src/yajhfc/console/Main.java:293
#, java-format
msgid "Invalid notification type {0}, using default instead."
msgstr "Ungültiger Benachrichtigungstyp {0}, verwende stattdessen Standardwert."

#: ../src/yajhfc/console/Main.java:305
#, java-format
msgid "Invalid paper size {0}, using default instead"
msgstr "Ungültige Papiergröße {0}, verwende stattdessen Standardwert."

#: ../src/yajhfc/console/Main.java:325
#, java-format
msgid "Invalid fax resolution {0}, using default instead"
msgstr "Ungültige Auflösung {0}, verwende stattdessen Standardwert."

#: ../src/yajhfc/console/Main.java:345
msgid "Invalid data specified for fax, exiting program."
msgstr "Ungültige Daten für Fax angegeben, beende Programm."

